{
  "manifest_version": 3,
  "name": "License Calculator Pro",
  "version": "1.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "permissions": ["storage", "clipboardWrite", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  }
}
